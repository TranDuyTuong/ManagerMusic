    <!--lọc theo mùa-->
    <div class="modal showmodal-seachNamemusic" tabindex="-1" id="ModalSeasonView">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Lọc Nhân Viên Theo Trạng Thái</h5>
                    <button type="button" class="btn-close" id="btn_closeSeason">X</button>
                </div>
                <div class="modal-body">
                    <div class="mb-3">
                        <label class="form-label" style="color: black; margin: 10px 10px;">Chọn Trạng Thái</label>
                        <select class="form-control" style="border-radius: 10px;">
                            <option selected value="0">Vui Lòng Chọn Trạng Thái ...</option>
                            <option value="1">Khóa Tài Khoản</option>
                            <option value="2">Không Bị Khóa</option>
                        </select>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" style="border-radius: 5px;">
                        <i class="fas fa-filter"></i> Lọc
                    </button>
                </div>
            </div>
        </div>
    </div>

    <!--lọc theo giới tính-->
    <div class="modal showmodal-seachNamemusic" tabindex="-1" id="ModalGenderView">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Lọc Nhân Viên Theo Giới Tính</h5>
                    <button type="button" class="btn-close" id="btn_closegender">X</button>
                </div>
                <div class="modal-body">
                    <div class="mb-3">
                        <label class="form-label" style="color: black; margin: 10px 10px;">Chọn Giới Tính</label>
                        <select class="form-control" style="border-radius: 10px;">
                            <option selected value="0">Vui Lòng Chọn Giới Tính ...</option>
                            <option value="1">Nam</option>
                            <option value="2">Nữ</option>
                        </select>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" style="border-radius: 5px;">
                        <i class="fas fa-filter"></i> Lọc
                    </button>
                </div>
            </div>
        </div>
    </div>

    <!--Modal Fuilter date request-->
    <div class="modal showmodal-seachNamemusic" tabindex="-1" id="Modal_ShowFuilterDate">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Chọn Ngày Muốn Lọc</h5>
                    <button type="button" class="btn-close" id="btn_CLoseFuilterDate">X</button>
                </div>
                <div class="modal-body">
                    <div class="mb-3">
                        <label class="form-label" style="font-size:15px;" id="TxtStartEnd">
                            Ngày bắt đầu đến ngày kết
                            thúc
                        </label>

                        <input name="daterange" class="form-control" type="text" id="rangeDate"
                               placeholder="Vui lòng chọn khoảng thời gian" data-input>
                    </div>
                </div>
                <div class="modal-footer">
                    <button style="border-radius: 5px;" id="btn_FuilterDate" type="button" class="btn btn-primary">
                        <i class="fas fa-filter"></i> Lọc Ngày
                    </button>
                </div>
            </div>
        </div>
    </div>

    <!--Modal Fuilter age staff-->
    <div class="modal showmodal-seachNamemusic" tabindex="-1" id="Modal_ShowFuilterAge">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Chọn Tuổi Muốn Lọc</h5>
                    <button type="button" class="btn-close" id="btn_CLoseFuilterAge">X</button>
                </div>
                <div class="modal-body">
                    <div class="mb-3">
                        <label class="form-label" style="font-size:15px;" id="TxtStart">Độ Tuổi Từ</label>
                        <input class="form-control" type="number" id="FromAge" placeholder="Nhập tuổi bắt đầu ..."
                               data-input>
                        <span style="color: red;" id="ErorrFrom"></span>
                    </div>
                    <div class="mb-3">
                        <label class="form-label" style="font-size:15px;" id="TxtStart">Đến Độ Tuổi</label>
                        <input class="form-control" type="number" id="EndAge" placeholder="Nhập tuổi kết thúc ..."
                               data-input>
                        <span style="color: red;" id="ErorrTo"></span>
                    </div>
                </div>
                <div class="modal-footer">
                    <button style="border-radius: 5px;" id="btn_FuilterAge" type="button" class="btn btn-primary">
                        <i class="fas fa-filter"></i> Lọc Danh Sách
                    </button>
                </div>
            </div>
        </div>
    </div>

    <!--Modal Fuilter address-->
    <div class="modal showmodal-seachNamemusic" tabindex="-1" id="Modal_ShowFuilterAddress">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Chọn Tp/Tỉnh, Huyện Muốn Lọc</h5>
                    <button type="button" class="btn-close" id="btn_CLoseFuilterAddress">X</button>
                </div>
                <div class="modal-body">
                    <div class="mb-3">
                        <label class="form-label" style="font-size:15px;" id="TxtStart">Tp/Tỉnh</label>
                        <select style="border-radius: 5px;" class="form-control" aria-label="Default select example">
                            <option selected value="0">Chọn Tp/Tỉnh ...</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>
                    <div class="mb-3">
                        <label class="form-label" style="font-size:15px;" id="TxtStart">Quận/Huyện</label>
                        <select style="border-radius: 5px;" class="form-control" aria-label="Default select example">
                            <option selected value="0">Chọn Quận/Huyện ...</option>
                            <option value="1">One</option>
                            <option value="2">Two</option>
                            <option value="3">Three</option>
                        </select>
                    </div>
                </div>
                <div class="modal-footer">
                    <button style="border-radius: 5px;" id="btn_FuilterAddress" type="button" class="btn btn-primary">
                        <i class="fas fa-filter"></i> Lọc Danh Sách
                    </button>
                </div>
            </div>
        </div>
    </div>

